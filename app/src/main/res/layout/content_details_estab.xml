<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.ferna.appcivico.VIEW.DetailsActivity"
    tools:showIn="@layout/activity_details">

    <FrameLayout
        android:id="@+id/frame"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

    </FrameLayout>

    <TextView
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="80dp"
        android:id="@+id/textViewNome"
        android:layout_below="@+id/frame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginLeft="10dp"
        android:id="@+id/tCategoria"
        android:layout_marginRight="80dp"
        android:layout_below="@id/textViewNome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginLeft="10dp"
        android:id="@+id/tCNPJ"
        android:layout_marginRight="80dp"
        android:layout_below="@id/tCategoria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:layout_marginRight="80dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/tAtendimento"
        android:layout_below="@+id/tCNPJ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <TextView
        android:layout_marginLeft="10dp"
        android:id="@+id/textView"
        android:layout_below="@id/frame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="59dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="20dp"
        android:layout_marginEnd="20dp"
        android:src="@mipmap/ic_phone_white"
        android:id="@+id/fabCall"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/ic_dialog_map"
        android:layout_alignBottom="@+id/tAtendimento"
        android:layout_alignLeft="@+id/fabCall"
        android:layout_alignStart="@+id/fabCall"
        android:id="@+id/fabGoMaps" />

    <android.support.design.widget.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="mini"
        app:srcCompat="@android:drawable/ic_dialog_info"
        android:layout_below="@+id/fabGoMaps"
        android:layout_alignRight="@+id/fabGoMaps"
        android:layout_alignEnd="@+id/fabGoMaps"
        android:layout_marginTop="19dp"
        android:id="@+id/fabServicos" />
</RelativeLayout>
